{
  "name": "mdx-blog",
  "version": "0.1.0",
  "engines": {
    "node": "^20.16.0",
    "npm": "^10.8.0"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "analyze:html": "ANALYZE=1 next build",
    "analyze:json": "ANALYZE=2 next build",
    "prisma:migrate:draft": "dotenv -e .env.local -- prisma migrate dev --create-only --name=${npm_config_name}",
    "prisma:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:studio": "dotenv -e .env.local -- prisma studio"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^14.2.15",
    "@prisma/client": "^5.21.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-devtools": "^5.59.16",
    "@toss/date": "^1.2.0",
    "@trpc/client": "^11.0.0-rc.528",
    "@trpc/next": "^11.0.0-rc.528",
    "@trpc/react-query": "^11.0.0-rc.528",
    "@trpc/server": "^11.0.0-rc.528",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "next": "14.2.15",
    "next-mdx-remote": "^5.0.0",
    "react": "^18",
    "react-dom": "^18",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "bugs": "https://github.com/ato-m-a/mdx-blog/issues",
  "homepage": "https://github.com/ato-m-a/mdx-blog#readme"
}
